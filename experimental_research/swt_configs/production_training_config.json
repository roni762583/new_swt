{
    "experiment_name": "swt_production_100k_episodes",
    "num_episodes": 100000,
    "batch_size": 128,
    "learning_rate": 0.0001,
    "weight_decay": 1e-5,
    "gradient_clip": 10.0,
    
    "market_encoder_config": {
        "wst": {
            "J": 2,
            "Q": 6,
            "input_length": 256,
            "market_output_dim": 128,
            "dropout_p": 0.1,
            "use_batch_norm": true,
            "use_residual": true
        },
        "fusion": {
            "position_features": 9,
            "combined_output_dim": 128,
            "fusion_hidden_dim": 256
        }
    },
    
    "muzero_config": {
        "market_wst_features": 128,
        "position_features": 9,
        "total_input_dim": 137,
        "final_input_dim": 128,
        "hidden_dim": 256,
        "representation_blocks": 3,
        "dynamics_blocks": 3,
        "prediction_blocks": 2,
        "afterstate_blocks": 2,
        "chance_space_size": 32,
        "chance_history_length": 4,
        "afterstate_enabled": true,
        "num_actions": 4,
        "support_size": 300,
        "dropout_rate": 0.1,
        "layer_norm": true,
        "residual_connections": true
    },
    
    "mcts_config": {
        "num_simulations": 15,
        "c_puct": 1.25,
        "discount": 0.997,
        "dirichlet_alpha": 0.25,
        "exploration_fraction": 0.25,
        "temperature": 1.0,
        "temperature_threshold": 30
    },
    
    "buffer_size": 100000,
    "min_buffer_size": 50,
    "eviction_batch": 1000,
    "min_quality_threshold": 0.05,
    "num_unroll_steps": 5,
    "td_steps": 10,
    
    "train_interval": 5,
    "eval_interval": 1000,
    "save_interval": 2000,
    "temperature_schedule": true,
    
    "value_loss_weight": 1.0,
    "policy_loss_weight": 1.0,
    "reward_loss_weight": 1.0,
    "kl_loss_weight": 0.05,
    
    "data_path": "swt_data/trading_db_muzero_pipeline.duckdb",
    "reward_type": "amddp5",
    
    "enable_curriculum": false,
    "enable_checkpoints": true,
    "save_frequency": 10,
    
    "multiprocessing": {
        "enabled": false,
        "num_workers": 8,
        "episodes_per_batch": 16
    },
    
    "log_level": "INFO",
    "save_dir": "swt_production_checkpoints",
    
    "docker": {
        "gpu": true,
        "memory_limit": "8g",
        "log_monitoring": true
    }
}